{
    "prConcurrentLimit": 5,
    "ignoreDeps": ["react", "react-dom"],
    "packageRules": [
        {
            "matchPackageNames": ["eslint-plugin-jest"],
            "allowedVersions": "!/^24\\.((1\\.3)|(1\\.5)|(2\\.2)|(3\\.2)|(3\\.4))$/"
        },
        {
            "matchPackageNames": ["y18n"],
            "allowedVersions": "!/^4\\.0\\.1$/"
        },
        {
            "matchPackageNames": ["eslint-config-prettier"],
            "allowedVersions": "!/^(7\\.2\\.0)|(8\\.1\\.0)$/"
        }
    ],
    "commitMessagePrefix": "chore",
    "rangeStrategy": "replace"
}
